{
    "name": "axelitus/asseter",
    "description": "Asseter is a framework agnostic PHP package to easily manage web assets (css, js, img,...) with useful features.",
    "type": "library",
    "keywords": ["asset", "manager", "css", "img", "js", "render"],
    "homepage": "http://axelitus.mx/projects/axelitus/asseter",
    "license": "MIT",
    "authors": [
        {
            "name": "Axel Pardemann",
            "email": "axelitusdev@gmail.com",
            "homepage": "http://axelitus.mx",
            "role": "Lead Developer"
        }
    ],
    "support": {
        "email": "axelitusdev@gmail.com",
        "source": "https://github.com/axelitus/php-asseter",
        "issues": "https://github.com/axelitus/php-asseter/issues",
        "wiki": "https://github.com/axelitus/php-asseter/wiki"
    },
    "require": {
        "php": ">=5.4.9",
        "axelitus/base": "dev-develop",
        "axelitus/design-patterns":"dev-develop"
    },
    "require-dev": {
        "phpunit/phpunit": "3.8.*",
        "squizlabs/php_codesniffer": "1.4.*",
        "phpmd/phpmd" : "1.5.*"
    },
    "autoload": {
        "psr-0": {
            "axelitus\\Asseter": "src/"
        }
    },
    "config": {
        "vendor-dir": "vendor"
    },
    "minimum-stability": "dev"
}
